}

.divError{
	width:30%; 
	margin:0 auto;
	border:1px solid red;
	font-size:20px;
	text-align:center;
	color:red;
	display:none;
}

.no_records_found
{
	width:90%;
	font-weight:bold;
	font-size:20px;
	display:none;
}

